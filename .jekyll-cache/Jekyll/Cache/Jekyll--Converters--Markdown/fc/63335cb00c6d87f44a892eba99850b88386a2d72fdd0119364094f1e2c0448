     <span class="k">this</span><span class="p">.</span><span class="nx">x</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>
     <span class="k">this</span><span class="p">.</span><span class="nx">fun1</span> <span class="o">=</span> <span class="kd">function</span><span class="p">(){</span>
         <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="k">this</span><span class="p">.</span><span class="nx">x</span><span class="p">)</span>
     <span class="p">}</span>
     <span class="nx">EasyEmitter</span><span class="p">.</span><span class="nx">call</span><span class="p">(</span><span class="k">this</span><span class="p">)</span> <span class="c1">// è°ƒç”¨äº†è¿™ä¸ªï¼Œthisä¸Šé¢å°±æœ‰EasyEmitteré‡Œé¢çš„æ–¹æ³•äº†ï¼Œå¦‚: emitæ–¹æ³•</span>
     <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="k">this</span><span class="p">)</span> <span class="c1">// MyFun{x: 1, fun1: f, emit: f}  myFunå¯¹è±¡ä¸Šï¼Œæœ‰EasyEmitteré‡Œé¢çš„æ–¹æ³•äº†</span>
     <span class="k">this</span><span class="p">.</span><span class="nx">emit</span><span class="p">(</span><span class="dl">'</span><span class="s1">ddd</span><span class="dl">'</span><span class="p">,</span><span class="kc">null</span><span class="p">)</span> <span class="c1">// EasyEmitteré‡Œé¢çš„thisï¼Œå°±æŒ‡å‘MyFunå¯¹è±¡äº†</span>
 <span class="p">}</span>

 <span class="kd">let</span> <span class="nx">myFun</span> <span class="o">=</span> <span class="k">new</span> <span class="nx">MyFun</span><span class="p">()</span>
 <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">myFun</span><span class="p">)</span> <span class="c1">// MyFun{x: 1, fun1: f, emit: f}  myFunå¯¹è±¡ä¸Šï¼Œæœ‰EasyEmitteré‡Œé¢çš„æ–¹æ³•äº†</span>


